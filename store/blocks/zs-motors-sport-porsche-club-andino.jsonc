  // Hero
  "flex-layout.row#zs-sport-banner": {
    "children": [
      "flex-layout.col#zs-sport-image-banner",
      "flex-layout.col#zs-sport-item-text-banner"
    ],
    "props": {
      "blockClass": "servicios-banner-row",
      "colSizing": "auto",
      "fullWidth": true
    }
  },
  "flex-layout.col#zs-sport-image-banner": {
    "children": ["image#zs-motors-sport"],
    "props": {
      "blockClass": "servicios-image-banner-col"
    }
  },
  "flex-layout.col#zs-sport-item-text-banner": {
    "children": ["rich-text#zs-sport-title-club-porsche"],
    "props": {
      "blockClass": "servicios-text-banner-col"
    }
  },
  "rich-text#zs-sport-title-club-porsche": {
    "props": {
      "text": "Porsche **Club Andino**",
      "blockClass": "servicios-title"
    }
  },

  "flex-layout.row#zs-motors-sport-image-text": {
    "children": [
      "flex-layout.col#zs-sport-text-title",
      "flex-layout.col#zs-sport-image"
    ],
    "props": {
      "blockClass": "zs-sport-row-image-text"
    }
  },
  "flex-layout.col#zs-sport-text-title": {
